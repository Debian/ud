<?xml version="1.0" ?>
<domain name="${"$${sip_profile}"}">
% for i, user in enumerate(users):
%     if user.is_not_retired() and user.has_active_password() and user.voipPassword:
  <user id="${user.uid}">
    <params>
      <param name="a1-hash" value="${user.voipPassword}"/>
    </params>
  </user>
%     endif
% endfor
</domain>
